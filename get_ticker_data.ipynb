    "headers = {\n",
    "    'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/58.0.3029.110 Safari/537.3'\n",
    "}\n",
    "response = requests.get(url, headers=headers)\n",
    "soup = BeautifulSoup(response.text, 'html.parser')\n",
    "\n",
    "# print (soup)\n",
    "# Example: Extract Face Value\n",
    "face_value = None\n",
    "face_value_li = soup.find(\"span\", class_=\"name\", string=lambda x: x and \"Face Value\" in x)\n",
    "if face_value_li:\n",
    "    value_span = face_value_li.find_next_sibling(\"span\", class_=\"nowrap value\")\n",
    "    number_span = value_span.find(\"span\", class_=\"number\")\n",
    "    face_value = number_span.get_text(strip=True) if number_span else None\n",
    "    print(\"Face Value:\", face_value)\n",
    "else:\n",
    "    print(\"Face Value not found.\")\n",
    "\n",
    "# Example: Extract Shareholding Pattern Table\n",
    "\n",
    "shareholding_section = soup.find('section', {'id': 'shareholding'})\n",
    "if table:\n",
    "    rows = []\n",
    "    for row in table.find_all('tr'):\n",
    "        cells = [cell.get_text(strip=True).replace('\\xa0+', '') for cell in row.find_all(['th', 'td'])]\n",
    "        rows.append(cells)\n",
    "    df = pd.DataFrame(rows[1:], columns=rows[0])\n",
    "    print(tabulate(df, headers='keys', tablefmt='grid', showindex=False))\n",
    "else:\n",
    "    print('Shareholding pattern section not found.')\n",
    "\n",
    "# You can similarly extract Debt and other values by inspecting the HTML structure."
