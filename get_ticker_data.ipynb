    "# Specify your ticker (example: 'RELIANCE.NS' for Reliance Industries on NSE)\n",
    "ticker_symbol = 'RELIANCE.NS'\n",
    "ticker = yf.Ticker(ticker_symbol)\n",
    "\n",
    "# Fetch basic info\n",
    "info = ticker.info\n",
    "\n",
    "# Market Cap\n",
    "market_cap = info.get('marketCap')\n",
    "# 52-week high/low\n",
    "week_52_high = info.get('fiftyTwoWeekHigh')\n",
    "week_52_low = info.get('fiftyTwoWeekLow')\n",
    "# All-time high/low (need to calculate from history)\n",
    "hist = ticker.history(period='max')\n",
    "all_time_high = hist['High'].max()\n",
    "all_time_low = hist['Low'].min()\n",
    "# Script P/E\n",
    "pe_ratio = info.get('trailingPE')\n",
    "# Sector P/E (not always available)\n",
    "sector_pe = info.get('forwardPE')\n",
    "# Face Value (rarely available)\n",
    "face_value = info.get('faceValue')\n",
    "\n",
    "print('Market Cap:', market_cap)\n",
    "print('52 Week High:', week_52_high)\n",
    "print('52 Week Low:', week_52_low)\n",
    "print('All Time High:', all_time_high)\n",
    "print('All Time Low:', all_time_low)\n",
    "print('Script P/E:', pe_ratio)\n",
    "print('Sector P/E:', sector_pe)\n",
    "print('Face Value:', face_value)"
   ]
